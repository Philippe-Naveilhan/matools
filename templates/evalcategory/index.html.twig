{% extends 'base.html.twig' %}

{% block title %}Evalcategory index{% endblock %}

{% block body %}
    <h1>Evalcategory index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for evalcategory in evalcategories %}
            <tr>
                <td>{{ evalcategory.id }}</td>
                <td>{{ evalcategory.name }}</td>
                <td>
                    <a href="{{ path('evalcategory_show', {'id': evalcategory.id}) }}">show</a>
                    <a href="{{ path('evalcategory_edit', {'id': evalcategory.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('evalcategory_new') }}">Create new</a>
{% endblock %}
